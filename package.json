{
  "name": "nectar",
  "version": "3.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4400",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng run nectar:serve-ssr",
    "serve:ssr": "node dist/nectar/server/main.js",
    "build:ssr": "ng build -c=production --aot --output-hashing=all && ng run nectar:server:production",
    "prerender": "ng run nectar:prerender",
    "deploy": "npm version patch && npm run prerender && npm run zip-build && npm run version:replace && eb deploy --profile=nectar && npm run version:return",
    "zip-build": "node zip-build.js",
    "version:replace": "replace npm_version %npm_package_version% .elasticbeanstalk/config.yml",
    "version:return": "replace %npm_package_version% npm_version .elasticbeanstalk/config.yml"
  },
  "exports": {
    ".": {
      "sass": "./styles/*.scss"
    }
  },
  "private": true,
  "dependencies": {
